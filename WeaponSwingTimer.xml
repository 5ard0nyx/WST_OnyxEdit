
<Ui xmlns="http://www.blizzard.com/wow/ui/"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Script file="WeaponSwingTimer.lua"/>
    <Frame name="WSTSwingFrame" hidden="true">
        <Scripts>
            <OnLoad>
                LeftHandedGlove_WST_OnLoad()
            </OnLoad>
            <OnUpdate>
                LeftHandedGlove_WST_OnUpdate(duration)
            </OnUpdate>
            <OnEvent>
                LeftHandedGlove_WST_OnEvent(event, ...)
            </OnEvent>
            
        </Scripts>
        <Size>
            <AbsDimension x="200" y="100"/>
        </Size>
        <Anchors>
            <Anchor point="CENTER"/>
        </Anchors>
        <Backdrop bgFile="Interface\CHARACTERFRAME\BarFill" tile="true">
            <BackgroundInsets>
				<AbsInset left="11" right="12" top="12" bottom="11"/>
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="32"/>
			</TileSize>
        </Backdrop>
    </Frame>
</Ui>
